Id: {\n        type: Number,\n        required: true,\n      },\n    },\n    data() {\n      return {\n        title: '',\n        content: '',\n      };\n    },\n    async created() {\n      try {\n        const response = await apiClient.get(`/diary/${this.diaryId}`);\n        const { title, content } = response.data;\n        this.title = title;\n        this.content = content;\n      } catch (error) {\n        console.error('Error fetching diary:', error.message);\n      }\n    },\n    methods: {\n      async submitUpdate() {\n        try {\n          await apiClient.put(`/diary/${this.diaryId}`, {\n            title: this.title,\n            content: this.content,\n          });\n          alert('Diary updated successfully!');\n          this.$router.push('/');\n        } catch (error) {\n          console.error('Error updating diary:', error.message);\n          alert('Failed to update diary. Please try again.');\n        }\n      },\n    },\n  };\n  </script>"],"mappings":";AAkBE,OAAOA,SAAQ,MAAO,iBAAiB;AAEvC,eAAe;EACbC,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMV,SAAS,CAACW,GAAG,CAAC,UAAU,IAAI,CAACT,OAAO,EAAE,CAAC;MAC9D,MAAM;QAAEK,KAAK;QAAEC;MAAQ,IAAIE,QAAQ,CAACJ,IAAI;MACxC,IAAI,CAACC,KAAI,GAAIA,KAAK;MAClB,IAAI,CAACC,OAAM,GAAIA,OAAO;IACxB,EAAE,OAAOI,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAACE,OAAO,CAAC;IACvD;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAMhB,SAAS,CAACiB,GAAG,CAAC,UAAU,IAAI,CAACf,OAAO,EAAE,EAAE;UAC5CK,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,OAAO,EAAE,IAAI,CAACA;QAChB,CAAC,CAAC;QACFU,KAAK,CAAC,6BAA6B,CAAC;QACpC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACxB,EAAE,OAAOR,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAACE,OAAO,CAAC;QACrDI,KAAK,CAAC,2CAA2C,CAAC;MACpD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"s